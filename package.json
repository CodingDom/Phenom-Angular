{
  "name": "phenom-angular",
  "version": "1.0.0",
  "description": "First angular project",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "ng build --prod"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodingDom/Phenom-Angular.git"
  },
  "keywords": [
    "Phenom",
    "Angular",
    "Basketball"
  ],
  "author": "Dominic Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodingDom/Phenom-Angular/issues"
  },
  "homepage": "https://github.com/CodingDom/Phenom-Angular#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "engines": {
    "node": "^6.11.0",
    "npm": "^6.4.1"
  }
}
